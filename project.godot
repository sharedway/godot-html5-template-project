; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="WebProject"
config/features=PackedStringArray("4.5", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

GlobalEventBus="*res://autoloaded/global_event_bus.gd"

[display]

window/size/viewport_width=1980
window/size/viewport_height=1080
window/vsync/vsync_mode=0

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
textures/vram_compression/import_etc2_astc=true
global_illumination/voxel_gi/quality=1
shading/overrides/force_vertex_shading=true
scaling_3d/scale=2.0
textures/default_filters/texture_mipmap_bias=1.0
